    "react-dom": "^18.2.0",
    "sirv": "^2.0.4"
  },
  "devDependencies": {
    "@tanstack/router-devtools": "^1.31.6",
    "@tanstack/router-vite-plugin": "^1.30.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
